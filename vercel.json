{
  "routes": [
    {
      "src": "^/assets/.*\\.css$",
      "headers": {
        "Content-Type": "text/css"
      },
      "continue": true
    },
    {
      "src": "^/assets/.*\\.(js|mjs)$",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "^/manifest\\.json$",
      "headers": {
        "Content-Type": "application/manifest+json",
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "dest": "/manifest.json"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate",
        "X-Frame-Options": "DENY",
        "X-Content-Type-Options": "nosniff"
      },
      "dest": "/index.html"
    }
  ],
  "cleanUrls": true
}
